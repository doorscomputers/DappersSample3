          <Series>
            <Simple>
              <Value Name="DataItem1" />
            </Simple>
          </Series>
        </Pane>
      </Panes>
    </Chart>
    <Pie ComponentName="pieDashboardItem1" Name="Pies 1" DataSource="dataSource1">
      <DataItems>
        <Dimension DataMember="catgory" Name="DataItem0" />
        <Measure DataMember="qty" Name="DataItem1">
