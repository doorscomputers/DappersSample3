      </DataItems>
      <GridColumns>
        <GridDimensionColumn Name="Date Delivered">
          <Dimension Name="DataItem0" />
        </GridDimensionColumn>
        <GridDimensionColumn Name="Items Bought">
          <Dimension Name="DataItem2" />
        </GridDimensionColumn>
        <GridDimensionColumn Name="Qty Bought">
